{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base", "eslint:recommended", "plugin:prettier/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": { "prettier/prettier": "error" }
}
